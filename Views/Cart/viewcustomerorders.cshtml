@{
    ViewBag.Title = "viewcustomerorders";
    Layout = "~/Views/Shared/_Layoutdish.cshtml";
}


<h1>Cart</h1>
<div id="content" class="float_r">
    <table width="680px" cellspacing="0" cellpadding="5">
        <tr bgcolor="#ddd">
            <th width="180" align="left">Order ID </th>
            <th width="100" align="center">Date </th>
            <th width="100" align="center">Amount </th>
            <th width="90"> </th>


        </tr>

        @foreach (var x in ViewBag.X)
        {
            <tr>
                <td align="center">@x.orderdetailId </td>
                <td align="right">Rs. @x.dateoforder </td>
                <td align="right">Rs. @x.Amount </td>
            </tr>
        }

    </table>
    <div style="float:right; width: 215px; margin-top: 20px;">
        <p><a href="/Cart/checkout">Proceed to checkout</a></p>

    </div>
</div>
